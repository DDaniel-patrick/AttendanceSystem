              <h1 class="hero-title global-title">
                <a href="#"><span><%= Details.name %> Attendance</span></a>
                
              </h1>
              <% let x =[] %>

              <% AllAttendee?.map(i=>{ %>
                  <% let v=JSON.stringify(i) %>
                  <% let y=JSON.parse(v) %>
                  
                  <% delete y["_id"] %>
                  <% delete y["__v"] %>

                  <% x.push(JSON.stringify(y)) %>
                <% }) %>
                <% AllAbsenseAttendee?.map(i=>{ %>
                  <% let v=JSON.stringify(i) %>
                  <% let y=JSON.parse(v) %>
                  
