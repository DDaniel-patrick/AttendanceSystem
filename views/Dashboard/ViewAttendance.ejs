                <% }) %>
              <h3 class="hero-title"><%= `${Details.closed?"Closed":"Open"}` %> || Created at <%= `${Details.createdAt}`.split('G')[0] %>|| <a style="cursor:pointer;font-weight: bolder;" id="downloadcsv" onclick="csvDownload([<%=x%>])">download</a> </h3>
              <!-- downloadcsv -->
              <script>
                function csvDownload(attendance) {
                  const ourData = attendance
                  

                const titleKeys = Object.keys(ourData[0])
                let refinedData = []
                refinedData.push(titleKeys)   
                ourData.forEach(item => {
                  refinedData.push(Object.values(item))  
                })
                let csvContent = ''

                  refinedData.forEach(row => {
                    csvContent += row.join(',') + '\n'
                  })
                  const blob = new Blob([csvContent], { type: 'text/csv;charset=utf-8,' })
                  const objUrl = URL.createObjectURL(blob)
                  
                  window.location.href=objUrl
                }
                
              </script>
