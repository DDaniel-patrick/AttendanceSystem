  <body class="home-template">
    <script>
      !(function () {
        "use strict";
        const t = document.body,
          o = getComputedStyle(t)
            .getPropertyValue("--ghost-accent-color")
            .trim();
        o && t.classList.add(getContrast(o));
      })();
    </script>
    <div class="global-wrap">
      <div
        class="global-cover"
        style="
          background-image: url(/content/images/size/w1600/2022/10/charlesdeluvio-cZr2sgaxy3Q-unsplash.jpg);
        "
      ></div>

      <div class="global-content">
        
        <main class="global-main">
          <div class="hero-section">
            <div class="hero-wrap global-padding">
              <h1 class="hero-title global-title">
                <span>Welcome to Come Work Home.</span>
              </h1>
              
            </div>
          </div>
          <div class="widget-section global-padding">
            <small class="global-subtitle">Where to go?</small>
            <div class="widget-wrap is-tags">
              <a
                href="/profile/view"
                class="item global-image-orientation global-radius"
              >
                <h2 class="item-title">Profile</h2>
                <div class="widget-image global-image">
                  <img
                    src="/content/images/size/w300/2022/10/photo-1642557581411-1be3a36d4786.jpg"
                    alt=""
                  />
                </div>
              </a>
              <a
                href="/attendance/add"
                class="item global-image-orientation global-radius"
              >
                <h2 class="item-title">Add Attendance</h2>
                <div class="widget-image global-image">
                  <img
                    src="/content/images/size/w300/2022/10/photo-1529307474719-3d0a417aaf8a.jpg"
                    alt=""
                  />
                </div>
              </a>
              <a
                href="/logout"
                class="item global-image-orientation global-radius"
              >
                <h2 class="item-title">Logout</h2>
                <div class="widget-image global-image">
                  <img
                    src="/content/images/size/w300/2022/10/photo-1434494878577-86c23bcb06b9.jpg"
                    alt=""
                  />
                </div>
              </a>
             
            </div>
          </div>
          <div class="loop-section global-padding">
            
            <small class="loop-subtitle global-subtitle">Previous posts</small>
            <div class="loop-wrap">
                <% allAttendance?.reverse()?.forEach(i => { %>
                    <article class="item">
                        <div
                          class="item-image global-image global-image-orientation global-radius"
                        >
                          <a
                            href="/attendance/view/<%= i._id %>"
                            class="global-link"
                            aria-label="We are stronger as a group than an individual"
                          ></a>
                          <img
                            srcset="
                              /content/images/size/w300/2022/10/rayul-_M6gy9oHgII-unsplash.jpg 300w,
                              /content/images/size/w600/2022/10/rayul-_M6gy9oHgII-unsplash.jpg 600w
                            "
                            sizes="(max-width:480px) 300px, 600px"
                            src="/content/images/size/w600/2022/10/rayul-_M6gy9oHgII-unsplash.jpg"
                            loading="lazy"
                            alt=""
                          />
                        </div>
                        <div class="item-content">
                          
                          <h2 class="item-title">
                            <a
                              href="/attendance/view/<%= i._id %>"
                              ><%= i.name %></a
                            >
                          </h2>
                          <p class="item-excerpt global-excerpt">
                            <%= i?.Bio %>
                          </p>
                          <div class="global-meta">
                            <div class="global-meta-content">
                              Created:
                              <a href="#"><%= `${(i?.createdAt)}`?.split('G')[0] %></a> <br> Closed:
                              <a href="#"><%= i.closed==true?`${i.updatedAt}`.split("G")[0]:"Open"
                                %></a>
                                <a href="/home/delete?id=<%= i._id %>">Delete</a>
                            </div>
                          </div>
                        </div>
                      </article>
                    <% }) %>
              
              
            </div>
          </div>
          

          
        </main>
       
      </div>
