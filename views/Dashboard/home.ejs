    <style>
      body.is-scroll-banner .is-banner {
        bottom: 30px;
      }
      .is-banner {
        font-size: 24px;
        position: fixed;
        z-index: 9999;
        right: 2%;
        bottom: -100px;
        display: inline-block;
        padding: 2px 10px;
        transition-timing-function: ease;
        transition-duration: 0.5s;
        transition-property: bottom, transform;
        color: #171822;
        border-radius: 100px;
        background-color: #97fe65;
      }
      .is-banner,
      .is-banner:hover {
